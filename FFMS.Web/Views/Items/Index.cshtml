        <a class="layui-btn layui-btn-normal layui-btn-xs" lay-event="bondTemplateEdit">编辑</a>
        <a class="layui-btn layui-btn-normal layui-btn-xs" lay-event="bondTemplateDlete">删除</a>
    </script>
    @section scripts{
        <script src="~/view-resources/Views/Items/Index.js" asp-append-version="true"></script>
